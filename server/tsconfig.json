{
  "compilerOptions":{
    "target": "ES2022",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "typeRoots": ["node_modules/@types"],
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
